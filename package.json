{
  "name": "@listener-js/log",
  "version": "1.38.0",
  "description": "Listener logger",
  "keywords": [
    "event",
    "listener",
    "logger"
  ],
  "author": "Listener <emit@listener.events>",
  "main": "dist/log.js",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/listener-js/log.git"
  },
  "license": "MIT",
  "homepage": "https://github.com/listener-js/log#readme",
  "scripts": {
    "build": "tsc",
    "fix": "npm run lint -- --fix",
    "lint": "eslint --ignore-path .gitignore --ext=js .",
    "prepublish": "rm -rf dist; npm run build",
    "pretty": "prettier --write **/*.css **/*.json **/*.md",
    "release": "./bin/release",
    "test": "mocha"
  },
  "devDependencies": {
    "@listener-js/listener": "3.5.0",
    "@types/mocha": "5.2.7",
    "@types/node": "12.11.5",
    "@typescript-eslint/eslint-plugin": "2.5.0",
    "@typescript-eslint/parser": "2.5.0",
    "acorn": "7.1.0",
    "eslint": "6.5.1",
    "eslint-config-prettier": "6.4.0",
    "eslint-plugin-prettier": "3.1.1",
    "expect": "24.9.0",
    "husky": "3.0.9",
    "lint-staged": "9.4.2",
    "mocha": "6.2.2",
    "prettier": "1.18.2",
    "release-it": "12.4.3",
    "rollup": "1.25.1",
    "semver": "6.3.0",
    "terser": "4.3.9",
    "ts-node": "8.4.1",
    "typescript": "3.6.4"
  },
  "prettier": {
    "printWidth": 60,
    "semi": false,
    "trailingComma": "es5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && npm test"
    }
  },
  "lint-staged": {
    "*.{css,json,md}": [
      "prettier --write",
      "git add"
    ],
    "*.{js,jsx,ts}": [
      "eslint --ignore-path .gitignore --fix",
      "git add"
    ]
  }
}
